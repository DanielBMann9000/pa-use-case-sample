{
  "title": "Version Adoption (top 10)",
  "type": "Line Graph",
  "input": "version-adoption.transformation>entities",
  "options": {
    "type": "stacked",
    "series_count": 10,
  	"series_sort": "-sum",
  	"xaxis": {
